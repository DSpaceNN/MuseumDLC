  Id: Soldier
  CharacterName: "\u0412\u043E\u0435\u043D\u043D\u044B\u0439"
  CharacterProf: "\u0428\u0442\u0443\u0440\u043C\u043E\u0432\u0438\u043A"
  WinDescription: "\u0412\u043E\u0435\u043D\u043D\u044B\u0439 \u2013 \u0447\u0435\u043B\u043E\u0432\u0435\u043A,
    \u043F\u0440\u0438\u043D\u0438\u043C\u0430\u044E\u0449\u0438\u0439 \u043D\u0435\u043F\u043E\u0441\u0440\u0435\u0434\u0441\u0442\u0432\u0435\u043D\u043D\u043E\u0435
    \u0443\u0447\u0430\u0441\u0442\u0438\u0435 \u0432 \u0441\u043F\u0435\u0446\u0438\u0430\u043B\u044C\u043D\u043E\u0439
    \u0432\u043E\u0435\u043D\u043D\u043E\u0439 \u043E\u043F\u0435\u0440\u0430\u0446\u0438\u0438.
    \u042D\u0442\u043E \u0442\u043E\u0442, \u043A\u0442\u043E \u043E\u0442\u0432\u0430\u0436\u043D\u043E
    \u0441\u0440\u0430\u0436\u0430\u0435\u0442\u0441\u044F \u043D\u0430 \u043F\u0435\u0440\u0435\u0434\u043E\u0432\u043E\u0439
    \u0441 \u0432\u0440\u0430\u0433\u0430\u043C\u0438 \u043D\u0430\u0448\u0435\u0439
    \u0420\u043E\u0434\u0438\u043D\u044B \u0440\u0430\u0434\u0438 \u043C\u0438\u0440\u043D\u043E\u0433\u043E
    \u043D\u0435\u0431\u0430 \u043D\u0430\u0434 \u0433\u043E\u043B\u043E\u0432\u043E\u0439.
    \u0423\u0447\u0430\u0441\u0442\u043D\u0438\u043A\u0438 \u0421\u0412\u041E - \u044D\u0442\u043E
    \u0441\u043E\u043B\u0434\u0430\u0442\u044B, \u043E\u0444\u0438\u0446\u0435\u0440\u044B,
    \u0434\u043E\u0431\u0440\u043E\u0432\u043E\u043B\u044C\u0446\u044B, \u0441 \u0447\u0435\u0441\u0442\u044C\u044E
    \u0438 \u0434\u043E\u0431\u043B\u0435\u0441\u0442\u044C\u044E \u0437\u0430\u0449\u0438\u0449\u0430\u044E\u0449\u0438\u0435
    \u0438\u043D\u0442\u0435\u0440\u0435\u0441\u044B \u043D\u0430\u0448\u0435\u0439
    \u0441\u0442\u0440\u0430\u043D\u044B. \u042D\u0442\u043E \u043D\u0430\u0441\u0442\u043E\u044F\u0449\u0438\u0435
    \u043F\u0430\u0442\u0440\u0438\u043E\u0442\u044B, \u043E\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0435\u043D\u043D\u043E
    \u0432\u044B\u043F\u043E\u043B\u043D\u044F\u044E\u0449\u0438\u0435 \u0432\u043E\u0438\u043D\u0441\u043A\u0438\u0439
    \u0434\u043E\u043B\u0433. \u0412\u043E\u0435\u043D\u043D\u044B\u0435 \u2013 \u043B\u044E\u0434\u0438,
    \u043A\u043E\u0442\u043E\u0440\u044B\u0435 \u043A\u0430\u0436\u0434\u044B\u0439
    \u0434\u0435\u043D\u044C \u043F\u043E\u043A\u0430\u0437\u044B\u0432\u0430\u044E\u0442
    \u043D\u0430\u043C \u043F\u0440\u0438\u043C\u0435\u0440\u044B \u0434\u043E\u0431\u043B\u0435\u0441\u0442\u0438
    \u0438 \u0433\u0435\u0440\u043E\u0438\u0437\u043C\u0430, \u0445\u0440\u0430\u0431\u0440\u043E\u0441\u0442\u0438
    \u0438 \u0441\u0438\u043B\u044B, \u043F\u0440\u0438 \u0432\u044B\u043F\u043E\u043B\u043D\u0435\u043D\u0438\u0438
    \u0431\u043E\u0435\u0432\u044B\u0445 \u0437\u0430\u0434\u0430\u0447 \u0432 \u0445\u043E\u0434\u0435
    \u0441\u043F\u0435\u0446\u0438\u0430\u043B\u044C\u043D\u043E\u0439 \u0432\u043E\u0435\u043D\u043D\u043E\u0439
    \u043E\u043F\u0435\u0440\u0430\u0446\u0438\u0438. \u042D\u0442\u043E \u043D\u0430\u0441\u0442\u043E\u044F\u0449\u0438\u0435
    \u0433\u0435\u0440\u043E\u0438 \u043D\u0430\u0448\u0435\u0433\u043E \u0432\u0440\u0435\u043C\u0435\u043D\u0438.
    \u0412\u043E\u0435\u043D\u043D\u044B\u0435 \u0440\u0435\u0448\u0430\u044E\u0442
    \u0441\u0430\u043C\u044B\u0435 \u0441\u043B\u043E\u0436\u043D\u044B\u0435 \u0437\u0430\u0434\u0430\u0447\u0438
    \u0432 \u0437\u043E\u043D\u0435 \u0431\u043E\u0435\u0432\u044B\u0445 \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u0439,
    \u0437\u0430\u0449\u0438\u0449\u0430\u044F \u043C\u0438\u0440\u043D\u044B\u0445
    \u0436\u0438\u0442\u0435\u043B\u0435\u0439 \u043D\u043E\u0432\u044B\u0445 \u0440\u0435\u0433\u0438\u043E\u043D\u043E\u0432.
    \u0411\u043E\u0440\u044E\u0442\u0441\u044F \u0441 \u043F\u043E\u0441\u043E\u0431\u043D\u0438\u043A\u0430\u043C\u0438
    \u043F\u0440\u0435\u0441\u0442\u0443\u043F\u043D\u043E\u0433\u043E \u043D\u0435\u043E\u043D\u0430\u0446\u0438\u0441\u0442\u0441\u043A\u043E\u0433\u043E
    \u0440\u0435\u0436\u0438\u043C\u0430. \u0423\u0447\u0430\u0441\u0442\u043D\u0438\u043A\u0430\u043C\u0438
    \u0421\u0412\u041E \u043C\u043E\u0433\u0443\u0442 \u0441\u0442\u0430\u0442\u044C
    \u043C\u0443\u0436\u0447\u0438\u043D\u044B \u0434\u043E 65 \u043B\u0435\u0442
    (\u0432\u044B\u0441\u0448\u0438\u0435 \u043E\u0444\u0438\u0446\u0435\u0440\u044B
    \u2013 \u0434\u043E 70). \u041A\u0430\u043D\u0434\u0438\u0434\u0430\u0442 \u0434\u043E\u043B\u0436\u0435\u043D
    \u0443\u0441\u043F\u0435\u0448\u043D\u043E \u043F\u0440\u043E\u0439\u0442\u0438
    \u043C\u0435\u0434\u0438\u0446\u0438\u043D\u0441\u043A\u0443\u044E \u043A\u043E\u043C\u0438\u0441\u0441\u0438\u044E,
    \u0438\u043C\u0435\u0442\u044C \u0432\u044B\u0441\u043E\u043A\u0438\u0439 \u0443\u0440\u043E\u0432\u0435\u043D\u044C
    \u0444\u0438\u0437\u0438\u0447\u0435\u0441\u043A\u043E\u0439 \u043F\u043E\u0434\u0433\u043E\u0442\u043E\u0432\u043A\u0438,
    \u0430 \u0442\u0430\u043A\u0436\u0435 \u043E\u043F\u044B\u0442 \u0432\u043E\u0435\u043D\u043D\u043E\u0439
    \u0441\u043B\u0443\u0436\u0431\u044B."
  Task: "\u041E\u0434\u0435\u0442\u044C \u0433\u0435\u0440\u043E\u044F\n\n\u041F\u043E\u0434\u0441\u043A\u0430\u0437\u043A\u0430:\r\n\u0417\u0430\u0436\u0438\u043C\u0430\u044F
    \u0438\u043A\u043E\u043D\u043A\u0443 \u043F\u0440\u0435\u0434\u043C\u0435\u0442\u0430,
    \u043F\u0435\u0440\u0435\u043D\u0435\u0441\u0438\u0442\u0435 \u0435\u0433\u043E
    \u043D\u0430 \u043C\u043E\u0434\u0435\u043B\u044C \u0433\u0435\u0440\u043E\u044F.
    \r\n\u0412\u0441\u0435 \u043F\u0440\u0435\u0434\u043C\u0435\u0442\u044B \u043A\u0430\u043A
    \u0438 \u0432 \u0440\u0435\u0430\u043B\u044C\u043D\u043E\u0441\u0442\u0438 \u0438\u043C\u0435\u044E\u0442
    \u043F\u0440\u0430\u0432\u0438\u043B\u044C\u043D\u0443\u044E \u043F\u043E\u0441\u043B\u0435\u0434\u043E\u0432\u0430\u0442\u0435\u043B\u044C\u043D\u043E\u0441\u0442\u044C
    \u043D\u043E\u0448\u0435\u043D\u0438\u044F. \u041E\u0434\u0435\u0432 \u043F\u0435\u0440\u0432\u0438\u0447\u043D\u044B\u0439
    \u043F\u0440\u0435\u0434\u043C\u0435\u0442, \u0432\u0430\u043C \u0431\u0443\u0434\u0435\u0442
    \u0440\u0430\u0437\u0431\u043B\u043E\u043A\u0438\u0440\u043E\u0432\u0430\u043D
    \u0441\u043B\u0435\u0434\u0443\u044E\u0449\u0438\u0439."
  Purpose: "\u041E\u0434\u0435\u0432\u0430\u044F \u0432\u044B\u0431\u0440\u0430\u043D\u043D\u043E\u0433\u043E
    \u0433\u0435\u0440\u043E\u044F \u0432\u044B \u0437\u043D\u0430\u043A\u043E\u043C\u0438\u0442\u0435\u0441\u044C
    \u0441 \u043E\u0441\u043E\u0431\u0435\u043D\u043D\u043E\u0441\u0442\u044F\u043C\u0438
    \u0435\u0433\u043E \u0440\u0430\u0431\u043E\u0442\u044B, \u043E\u0431\u043C\u0443\u043D\u0434\u0438\u0440\u043E\u0432\u0430\u043D\u0438\u044F
    \u0438 \u043E\u0442\u043B\u0438\u0447\u0438\u0442\u0435\u043B\u044C\u043D\u044B\u0445
    \u0445\u0430\u0440\u0430\u043A\u0442\u0435\u0440\u0438\u0441\u0442\u0438\u043A."
